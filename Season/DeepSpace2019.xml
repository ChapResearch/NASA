  <metaData>

    <field>
      <name>totalPenalties</name>
      <type>int</type>
      <label>Total Penalties</label>
      <perspective>match</perspective>
      <op>sum</op>
      <target>gamePieceOffRocket</target>
      <target>gamePieceOffCargo</target>
      <target>defensivePenalty</target>
    </field>


    <field> <!-- Panel Pickup -->
      <name>panelPickup</name>
      <type>event</type>
      <label>Combined pickup events</label>
      <perspective>match</perspective>
      <op>combine</op>
      <target>panelPickupGround</target>
      <target>panelPickupHuman</target>
    </field>

    <field> <!-- Combine the raw data (times) for the Rocket panel scored events -->
      <name>panelScoredRocket</name>
      <type>event</type>
      <label>Combined panel rocket scoring events</label>
      <perspective>match</perspective>
      <op>combine</op>
      <target>panelScoredRocketLvl1</target>
      <target>panelScoredRocketLvl2</target>
      <target>panelScoredRocketLvl3</target>
    </field>

    <field> <!-- Combine raw data for Cargo panel Scored events -->
      <name>panelScoredCargo</name>
      <type>event</type>
      <label>Combined panel cargo scoring events</label>
      <perspective>match</perspective>
      <op>combine</op>
      <target>panelScoredCargoMiddle</target>
      <target>panelScoredCargoRight</target>
      <target>panelScoredCargoLeft</target>
    </field>

    
    <!-- METADATA FOR CARGO PANEL SCORING-->

    <field> <!-- Times for Cargo based on Panel Pickup events and any Panel score on Cargo -->
      <name>panelScoredCargoTime</name>
      <type>event</type>
      <label>Cargo scoring times</label>
      <perspective>match</perspective>
      <op>delta</op>
      <start>panelPickup</start>
      <end>panelScoredCargo</end>
    </field>

    <field> <!-- Counts of number of times a panel was scored on cargo based on the panelScoredCargoTime -->
      <name>panelScoredCargoCount</name>
      <type>int</type>
      <label>Cargo scoring counts</label>
      <perspective>match</perspective>
      <op>count</op>
      <target>panelScoredCargoTime</target>
